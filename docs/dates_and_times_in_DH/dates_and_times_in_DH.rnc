  ## (epigraph) contains a quotation, anonymous or attributed, appearing at the start or end of a section or on a title page. [4.2.3. Arguments, Epigraphs, and Postscripts 4.2. Elements Common to All Divisions 4.6. Title Pages]
  element tei:epigraph {
    (dat_model.common | dat_model.global)*,
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    empty
  }

dat_opener =
  ## (opener) groups together dateline, byline, salutation, and similar phrases appearing as a preliminary group at the start of a division, especially of a letter. [4.2. Elements Common to All Divisions]
  element tei:opener {
    (text
     | dat_model.gLike
     | dat_model.phrase
     | dat_argument
     | dat_byline
     | dat_dateline
     | dat_epigraph
     | dat_salute
     | dat_signed
     | dat_model.global)*,
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.written.attributes,
    empty
  }

dat_closer =
  ## (closer) groups together salutations, datelines, bylines, and similar phrases appearing as a final group at the end of a division, especially of a letter. [4.2.2. Openers and Closers 4.2. Elements Common to All Divisions]
  element tei:closer {
    (text
     | dat_model.gLike
     | dat_byline
     | dat_signed
     | dat_dateline
     | dat_salute
     | dat_model.phrase
     | dat_model.global)*,
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.written.attributes,
    empty
  }

dat_salute =
  ## (salutation) contains a salutation or greeting prefixed to a foreword, dedicatory epistle, or other division of a text, or the salutation in the closing of a letter, preface, etc. [4.2.2. Openers and Closers]
  element tei:salute {
    dat_macro.paraContent,
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.written.attributes,
    empty
  }

dat_signed =
  ## (signature) contains the closing salutation, etc., appended to a foreword, dedicatory epistle, or other division of a text. [4.2.2. Openers and Closers]
  element tei:signed {
    dat_macro.paraContent,
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.written.attributes,
    empty
  }

dat_postscript =
  ## contains a postscript, e.g. to a letter. [4.2. Elements Common to All Divisions]
  element tei:postscript {
    ((dat_model.global | dat_model.divTopPart)*,
     dat_model.common,
     (dat_model.global | dat_model.common)*,
     (dat_model.divBottomPart, dat_model.global*)*),
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.placement.attributes,
    dat_att.written.attributes,
    empty
  }

dat_titlePage =
  ## (title page) contains the title page of a text, appearing within the front or back matter. [4.6. Title Pages]
  element tei:titlePage {
    (dat_model.global*,
     dat_model.titlepagePart,
     (dat_model.titlepagePart | dat_model.global)*),
    dat_att.global.attributes,
    dat_att.typed.attribute.subtype,
    ## classifies the title page according to any convenient typology.
    attribute type {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }
    }?,
    empty
  }

dat_docTitle =
  ## (document title) contains the title of a document, including all its constituents, as given on a title page. [4.6. Title Pages]
  element tei:docTitle {
    (dat_model.global*, (dat_titlePart, dat_model.global*)+),
    dat_att.global.attributes,
    dat_att.canonical.attributes,
    empty
  }

dat_titlePart =
  ## (title part) contains a subsection or division of the title of a work, as indicated on a title page. [4.6. Title Pages]
  element tei:titlePart {
    dat_macro.paraContent,
    dat_att.global.attributes,
    dat_att.typed.attribute.subtype,
    ## (type) specifies the role of this subdivision of the title.
    ## Suggested values include: 1] main (main); 2] sub (subordinate); 3] alt (alternate); 4] short (short); 5] desc (descriptive)
    [ a:defaultValue = "main" ]
    attribute type {
      ## (main) main title of the work
      "main" | ## (subordinate) subtitle of the work
        "sub" | ## (alternate) alternative title of the work
        "alt" | ## (short) abbreviated form of title
        "short" | ## (descriptive) descriptive paraphrase of the work
        "desc"
      | xsd:token { pattern = "[^\p{C}\p{Z}]+" }
    }?,
    empty
  }

dat_docAuthor =
  ## (document author) contains the name of the author of the document, as given on the title page (often but not always contained in a byline). [4.6. Title Pages]
  element tei:docAuthor {
    dat_macro.phraseSeq,
    dat_att.global.attributes,
    dat_att.canonical.attributes,
    dat_att.cmc.attributes,
    empty
  }

dat_imprimatur =
  ## (imprimatur) contains a formal statement authorizing the publication of a work, sometimes required to appear on a title page or its verso. [4.6. Title Pages]
  element tei:imprimatur {
    dat_macro.paraContent, dat_att.global.attributes, empty
  }

dat_docEdition =
  ## (document edition) contains an edition statement as presented on a title page of a document. [4.6. Title Pages]
  element tei:docEdition {
    dat_macro.paraContent, dat_att.global.attributes, empty
  }

dat_docImprint =
  ## (document imprint) contains the imprint statement (place and date of publication, publisher name), as given (usually) at the foot of a title page. [4.6. Title Pages]
  element tei:docImprint {
    (text
     | dat_model.gLike
     | dat_model.phrase
     | dat_pubPlace
     | dat_docDate
     | dat_publisher
     | dat_sponsor
     | dat_model.global)*,
    dat_att.global.attributes,
    empty
  }

dat_docDate =
  ## (document date) contains the date of a document, as given on a title page or in a dateline. [4.6. Title Pages]
  element tei:docDate {
    dat_macro.phraseSeq,
    dat_att.global.attributes,
    dat_att.calendarSystem.attributes,
    dat_att.cmc.attributes,
    dat_att.datable.attributes,
    empty
  }

dat_front =
  ## (front matter) contains any prefatory matter (headers, abstracts, title page, prefaces, dedications, etc.) found at the start of a document, before the main body. [4.6. Title Pages 4. Default Text Structure]
  element tei:front {
    ((dat_model.frontPart
      | dat_model.pLike
      | dat_model.pLike.front
      | dat_model.global)*,
     (((dat_model.div1Like,
        (dat_model.div1Like | dat_model.frontPart | dat_model.global)*)
       | (dat_model.divLike,
          (dat_model.divLike
           | dat_model.frontPart
           | dat_model.global)*)),
      (dat_model.divBottom,
       (dat_model.divBottom | dat_model.global)*)?)?),
    dat_att.global.attributes,
    dat_att.declaring.attributes,
    empty
  }

dat_back =
  ## (back matter) contains any appendixes, etc. following the main part of a text. [4.7. Back Matter 4. Default Text Structure]
  element tei:back {
    ((dat_model.frontPart
      | dat_model.pLike.front
      | dat_model.pLike
      | dat_model.listLike
      | dat_model.global)*,
     ((dat_model.div1Like,
       (dat_model.frontPart | dat_model.div1Like | dat_model.global)*)
      | (dat_model.divLike,
         (dat_model.frontPart
          | dat_model.divLike
          | dat_model.global)*))?,
     (dat_model.divBottomPart,
      (dat_model.divBottomPart | dat_model.global)*)?),
    dat_att.global.attributes,
    dat_att.declaring.attributes,
    empty
  }
dat_att.global.linking.attributes =
  dat_att.global.linking.attribute.corresp,
  dat_att.global.linking.attribute.synch,
  dat_att.global.linking.attribute.sameAs,
  dat_att.global.linking.attribute.copyOf,
  dat_att.global.linking.attribute.next,
  dat_att.global.linking.attribute.prev,
  dat_att.global.linking.attribute.exclude,
  dat_att.global.linking.attribute.select
dat_att.global.linking.attribute.corresp =
  ## (corresponds) points to elements that correspond to the current element in some way.
  attribute corresp {
    list {
      xsd:anyURI { pattern = "\S+" }+
    }
  }?
dat_att.global.linking.attribute.synch =
  ## (synchronous) points to elements that are synchronous with the current element.
  attribute synch {
    list {
      xsd:anyURI { pattern = "\S+" }+
    }
  }?
dat_att.global.linking.attribute.sameAs =
  ## points to an element that is the same as the current element.
  attribute sameAs {
    xsd:anyURI { pattern = "\S+" }
  }?
dat_att.global.linking.attribute.copyOf =
  ## points to an element of which the current element is a copy.
  attribute copyOf {
    xsd:anyURI { pattern = "\S+" }
  }?
dat_att.global.linking.attribute.next =
  ## (next) points to the next element of a virtual aggregate of which the current element is part.
  attribute next {
    xsd:anyURI { pattern = "\S+" }
  }?
dat_att.global.linking.attribute.prev =
  ## (previous) points to the previous element of a virtual aggregate of which the current element is part.
  attribute prev {
    xsd:anyURI { pattern = "\S+" }
  }?
dat_att.global.linking.attribute.exclude =
  ## points to elements that are in exclusive alternation with the current element.
  attribute exclude {
    list {
      xsd:anyURI { pattern = "\S+" }+
    }
  }?
dat_att.global.linking.attribute.select =
  ## selects one or more alternants; if one alternant is selected, the ambiguity or uncertainty is marked as resolved. If more than one alternant is selected, the degree of ambiguity or uncertainty is marked as reduced by the number of alternants not selected.
  attribute select {
    list {
      xsd:anyURI { pattern = "\S+" }+
    }
  }?

dat_ab =
  ## (anonymous block) contains any component-level unit of text, acting as a container for phrase or inter level elements analogous to, but without the same constraints as, a paragraph. [17.3. Blocks, Segments, and Anchors]
  element tei:ab {
    dat_macro.abContent
    >> sch:pattern [
         id =
           "dates_and_times_in_DH-ab-abstractModel-structure-ab-in-l-constraint-rule-30"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:l//tei:ab"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "ancestor::tei:floatingText | parent::tei:figure | parent::tei:note"
             "\x{a}" ~
             "          Abstract model violation: Metrical lines may not contain higher-level divisions such as p or ab, unless ab is a child of figure or note, or is a descendant of floatingText.\x{a}" ~
             "        "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    dat_att.global.attributes,
    dat_att.cmc.attributes,
    dat_att.declaring.attributes,
    dat_att.fragmentable.attributes,
    dat_att.typed.attribute.subtype,
    dat_att.written.attributes,
    attribute type {
      [
        a:documentation [
          "(formal data block) typically contains a sequence of "
          ns1:code [ "<code>" ]
          " children, each of which is intended to be displayed as what CSS calls a block."
        ]
      ]
      "fdb"
    }?,
    empty
  }
dat_att.datable.custom.attributes =
  dat_att.datable.custom.attribute.when-custom,
  dat_att.datable.custom.attribute.notBefore-custom,
  dat_att.datable.custom.attribute.notAfter-custom,
  dat_att.datable.custom.attribute.from-custom,
  dat_att.datable.custom.attribute.to-custom,
  dat_att.datable.custom.attribute.datingPoint,
  dat_att.datable.custom.attribute.datingMethod
dat_att.datable.custom.attribute.when-custom =
  ## supplies the value of a date or time in some custom standard form.
  attribute when-custom {
    list {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }+
    }
  }?
dat_att.datable.custom.attribute.notBefore-custom =
  ## specifies the earliest possible date for the event in some custom standard form.
  attribute notBefore-custom {
    list {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }+
    }
  }?
dat_att.datable.custom.attribute.notAfter-custom =
  ## specifies the latest possible date for the event in some custom standard form.
  attribute notAfter-custom {
    list {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }+
    }
  }?
dat_att.datable.custom.attribute.from-custom =
  ## indicates the starting point of the period in some custom standard form.
  attribute from-custom {
    list {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }+
    }
  }?
dat_att.datable.custom.attribute.to-custom =
  ## indicates the ending point of the period in some custom standard form.
  attribute to-custom {
    list {
      xsd:token { pattern = "[^\p{C}\p{Z}]+" }+
    }
  }?
dat_att.datable.custom.attribute.datingPoint =
  ## supplies a pointer to some location defining a named point in time with reference to which the datable item is understood to have occurred.
  attribute datingPoint {
    xsd:anyURI { pattern = "\S+" }
  }?
dat_att.datable.custom.attribute.datingMethod =
  [
    a:documentation [
      "supplies a pointer to a "
